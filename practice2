class Main {
    public static void main(String[] args) {
        int sum=0;
        int no=153;
        int original = no;
        
        while(no>0)
        {
            int rem = no%10;
            sum = sum+(rem*rem*rem);
            no=no/10;
        }
        
        if(original==sum)
         {
             System.out.println("Amstrong number ");
             
         }else{
             System.out.println("Not Amstrong number ");
         }
         
    }
}
