import java.io.*;


public class Filehandling {
    public static void main(String[] args) {
        File f = new File("C:\\Users\\kenga\\swapnil12.txt");
        try{
            if(f.createNewFile()){
                System.out.println("created");
            }
            else{
                System.out.println("not created");
            }
        }catch(IOException e){
            System.out.println("error");
        }

    }
}
